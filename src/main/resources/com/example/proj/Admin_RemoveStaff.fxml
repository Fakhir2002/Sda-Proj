<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #014D4E;" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proj.Admin_RemoveStaffController">
    <children>
        <Label layoutX="222.0" layoutY="27.0" text="Remove Staff" textFill="WHITE">
            <font>
                <Font name="System Bold" size="21.0" />
            </font>
            <effect>
                <Bloom />
            </effect>
        </Label>
        <Text fill="WHITE" layoutX="82.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Staff to remove">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Text>
        <Button fx:id="remm" layoutX="222.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleRemoveStaff" prefHeight="31.0" prefWidth="65.0" text="Remove" />
        <Button fx:id="staffback" layoutX="339.0" layoutY="187.0" mnemonicParsing="false" onAction="#handlestaffback" prefHeight="31.0" prefWidth="56.0" text="Back" />
      <ComboBox fx:id="removestaffcombobox" layoutX="261.0" layoutY="115.0" prefWidth="150.0" />
    </children>
</AnchorPane>
